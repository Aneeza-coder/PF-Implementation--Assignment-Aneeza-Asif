#include <iostream>
using namespace std;
int main()
{
   int a=19, b=90, temp;
   cout<<" Before swapping: "<<endl;
   cout<<" the value of a: "<<a<<endl;
   cout<<" the value of b: "<<b<<endl;
   temp=a;
   a=b;
   b=temp;
   cout<<" After swapping: "<<endl;
   cout<<" the value of a: "<<a<<endl;
   cout<<" the value of b: "<<b<<endl;
   return 0;
}
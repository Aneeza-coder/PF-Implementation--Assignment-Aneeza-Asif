#include <iostream>
using namespace std;
int main()
{
    int roll_number;
    float PF,CG,IC, total,average;
    cout<<"Enter your roll_number: ";
    cin>>roll_number;
    cout<<"Enter marks of PF: ";
    cin>>PF;
    cout<<"Enter marks of IC: ";
    cin>>IC;
    cout<<"Enter marks of CG: ";
    cin>>CG;
    total=PF+IC+CG;
    average=(PF+IC+CG)/3.0;
    cout<<"\n Total Marks= " <<total<<endl;
    cout<<"\n Average Marks= " <<average<<endl;
    return 0;
}
#include <iostream>
using namespace std;

int main()
{
    int age;
    float height;
    char name[50];
    char gender;

    cout << "Enter your name: ";
    cin >> name;

    cout << "Enter your age: ";
    cin >> age;

    cout << "Enter your height (in cm): ";
    cin >> height;

    cout << "Enter your gender (M/F): ";
    cin >> gender;

    cout << "\nStudent Details\n";
    cout << "Name: " << name << endl;
    cout << "Age: " << age << endl;
    cout << "Height: " << height << " cm" << endl;
    cout << "Gender: " << gender << endl;

    return 0;
}

#include <iostream>
#define PI 3.1416
using namespace std;
int main()
{
    float area ,radius ,circumference;
    cout<<"Enter radius of cylinder of the circle:";
    cin>>radius;
    area=3.1416*radius*radius;
    circumference=2*PI*radius;
    cout<<"The area of cylinder is: "<<area<<endl;
    cout<<"The circumference of cylinder is: "<<circumference<<endl;
    return 0;
}
#include <iostream>
using namespace std;

int main() {
    int ageYears = 20;
    int ageMonths;
    int ageDays;

    ageMonths = ageYears * 12;     // Converting years to months
    ageDays = ageYears * 365;      // Converting years to days (approx)

    cout << "Age in years: " << ageYears << endl;
    cout << "Age in months: " << ageMonths << endl;
    cout << "Age in days: " << ageDays << endl;

    return 0;
}
